{
  "appId": "com.apostoldelapalabra",
  "appName": "Apostol",
  "webDir": "www",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https"
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true
  },
  "plugins": {
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["google.com"]
    },
    "FirebaseFirestore": {
      "emulatorHost": null,
      "emulatorPort": null
    },
    "FirebaseRemoteConfig": {
      "minimumFetchIntervalInSeconds": 3600
    },
    "FirebaseStorage": {
      "emulatorHost": null,
      "emulatorPort": null
    }
  }
}
